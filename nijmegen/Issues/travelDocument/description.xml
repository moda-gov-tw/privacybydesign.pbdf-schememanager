<IssueSpecification version="4">
	<Name>
		<en>Travel Document</en>
		<nl>Reisdocument</nl>
		<zh>旅行證件</zh>
	</Name>
	<ShortName>
		<en>Travel Document</en>
		<nl>Reisdocument</nl>
		<zh>旅行證件</zh>
	</ShortName>
	<SchemeManager>pbdf</SchemeManager>
	<IssuerID>nijmegen</IssuerID>
	<CredentialID>travelDocument</CredentialID>
	<Description>
		<en>Your Dutch passport or identity card, from the Dutch population register</en>
		<nl>Je Nederlandse paspoort of identiteitskaart, afkomstig uit de Nederlandse Basisregistratie Persoonsgegevens</nl>
		<zh>您由荷蘭人口登記處所發放的荷蘭護照或身分證</zh>
	</Description>
	<ShouldBeSingleton>false</ShouldBeSingleton>
	<IssueURL>
		<en>https://services.nijmegen.nl/irma/issue/start</en>
		<nl>https://services.nijmegen.nl/irma/issue/start</nl>
		<zh>https://services.nijmegen.nl/irma/issue/start</zh>
	</IssueURL>

	<Attributes>
		<Attribute id="kind">
			<Name>
				<en>Document kind</en>
				<nl>Soort document</nl>
				<zh>文件類型</zh>
			</Name>
			<Description>
				<en>The document kind (passport or identity card)</en>
				<nl>Het soort reisdocument (paspoort of identiteitskaart)</nl>
				<zh>文件類型（護照或身分證）</zh>
			</Description>
		</Attribute>
		<Attribute id="number">
			<Name>
				<en>Document number</en>
				<nl>Documentnummer</nl>
				<zh>文件號碼</zh>
			</Name>
			<Description>
				<en>The travel document number</en>
				<nl>Het documentnummer</nl>
				<zh>旅行證件號碼</zh>
			</Description>
		</Attribute>
		<Attribute id="issuancedate">
			<Name>
				<en>Issuance date</en>
				<nl>Datum uitgifte</nl>
				<zh>發照/證日期</zh>
			</Name>
			<Description>
				<en>Date of issuance of the travel document</en>
				<nl>Datum van uitgifte van het reisdocument</nl>
				<zh>旅行證件發放日期</zh>
			</Description>
		</Attribute>
		<Attribute id="expirydate">
			<Name>
				<en>Expiry date</en>
				<nl>Verloopdatum</nl>
				<zh>到期日期</zh>
			</Name>
			<Description>
				<en>Expiry date of the travel document</en>
				<nl>Verloopdatum van het reisdocument</nl>
				<zh>旅行證件到期日期</zh>
			</Description>
		</Attribute>
		<Attribute id="documentissuer" optional="true">
			<Name>
				<en>Document issuer</en>
				<nl>Uitgever</nl>
				<zh>文件發照單位</zh>
			</Name>
			<Description>
				<en>Issuer of the travel document</en>
				<nl>Autoriteit van afgifte reisdocument</nl>
				<zh>旅行證件發照單位</zh>
			</Description>
		</Attribute>
	</Attributes>
</IssueSpecification>